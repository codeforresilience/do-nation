        	</StackPanel>
        	<StackPanel x:Name="itemButts" Margin="10,0,10,10" VerticalAlignment="Bottom" Height="647" HorizontalAlignment="Center">
        		<StackPanel Orientation="Horizontal" Margin="0,22,0,0">
        			<Button x:Name="btn_Noodle" Content="Noodles" Height="120" Margin="0,0,20,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="120" Style="{StaticResource noodles}" BorderThickness="0" Click="clickedGoods">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/noodle.png"/>
        				</Button.Background>
        			</Button>
        			<Button x:Name="btn_CannedGood" Content="Canned Goods" Height="120" Margin="0,0,20,0" VerticalAlignment="Center" Width="120" Style="{StaticResource cannedGoods}" BorderThickness="0" Click="clickedGoods" HorizontalAlignment="Center">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/cannedGood.png"/>
        				</Button.Background>
        			</Button>
        			<Button x:Name="btn_drinks" Content="Milk, Coffee, Drinks" Height="120" Margin="0" HorizontalAlignment="Right" VerticalAlignment="Center" Width="120" Style="{StaticResource milk}" BorderThickness="0" Click="clickedGoods" FontSize="22.667">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/milk.png"/>
        				</Button.Background>
        			</Button>
        		</StackPanel>
        		<StackPanel Orientation="Horizontal" Margin="0,22,0,0">
        			<Button x:Name="btn_Rice" Content="Rice" Height="120" Margin="0,0,20,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="120" Style="{StaticResource rice}" BorderThickness="0" Click="clickedGoods">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/rice.png"/>
        				</Button.Background>
        			</Button>
        			<Button x:Name="btn_water" Content="Water" Height="120" Margin="0,0,20,0" VerticalAlignment="Center" Width="120" Style="{StaticResource water}" BorderThickness="0" Click="clickedGoods" HorizontalAlignment="Center">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/water.png"/>
        				</Button.Background>
        			</Button>
        			<Button x:Name="btn_ReadyToEat" Content="Ready-to-eat Foods" Height="120" Margin="0" HorizontalAlignment="Right" VerticalAlignment="Center" Width="120" Style="{StaticResource readytoteat}" BorderThickness="0" Click="clickedGoods">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/readytoeat.png"/>
        				</Button.Background>
        			</Button>
        		</StackPanel>
        		<StackPanel Orientation="Horizontal" Margin="0,22,0,0">
        			<Button x:Name="btn_soaps" Content="Hygiene Supplies" Height="120" Margin="0,0,20,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="120" Style="{StaticResource hygiene}" BorderThickness="0" Click="clickedGoods">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/soap.png"/>
        				</Button.Background>
        			</Button>
        			<Button x:Name="btn_medsup" Content="Medical Supplies" Height="120" Margin="0,0,20,0" VerticalAlignment="Center" Width="120" Style="{StaticResource med}" BorderThickness="0" Click="clickedGoods" HorizontalAlignment="Center">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/medicalSupplies.png"/>
        				</Button.Background>
        			</Button>
        			<Button x:Name="btn_clothes" Content="Clothes" Height="120" Margin="0" HorizontalAlignment="Right" VerticalAlignment="Center" Width="120" Style="{StaticResource clothes}" BorderThickness="0" Click="clickedGoods">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/shirt.png"/>
        				</Button.Background>
        			</Button>
        		</StackPanel>
        		<StackPanel Orientation="Horizontal" Margin="0,22,0,0">
        			<Button x:Name="btn_Utensils" Content="Utensils" Height="120" Margin="0,0,20,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="120" Style="{StaticResource utensils}" BorderThickness="0" Click="clickedGoods">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/utensils.png"/>
        				</Button.Background>
        			</Button>
        			<Button x:Name="btn_vehicles" Content="Vehicles/Equipment" Height="120" Margin="0,0,20,0" VerticalAlignment="Center" Width="120" Style="{StaticResource vehicle}" BorderThickness="0" Click="clickedGoods" HorizontalAlignment="Center">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/vehicle.png"/>
        				</Button.Background>
        			</Button>
        			<Button x:Name="btn_conssupplies" Content="Building Materials" Height="120" Margin="0" HorizontalAlignment="Right" VerticalAlignment="Center" Width="120" Style="{StaticResource itemBtn}" BorderThickness="0" FontSize="21.333" Click="clickedGoods">
        				<Button.Background>
        					<ImageBrush Stretch="Fill" ImageSource="/Assets/icons_goods/tools.png"/>
        				</Button.Background>
        			</Button>
        		</StackPanel>
        	</StackPanel>
        </Grid>
        <Grid x:Name="Page2" Grid.RowSpan="2" Background="#FF3498DB" Visibility="Collapsed">

        	<StackPanel Grid.Row="0" Margin="11,17,-11,645" Orientation="Horizontal">
        		<TextBlock Margin="0" Style="{StaticResource PhoneTextTitle1Style}" Text="Donate" HorizontalAlignment="Left"/>
        		<TextBlock TextWrapping="Wrap" HorizontalAlignment="Right" Margin="156,0,0,0" Foreground="#FFACE5FF">
        			<Run FontSize="72" FontFamily="Segoe WP SemiLight" Text="2"/>
        			<Run FontSize="40" FontFamily="Segoe WP SemiLight" Text="/2"/>
        		</TextBlock>
        	</StackPanel>

        	<Grid x:Name="ContentPanel1" Margin="12,105,12,0" HorizontalAlignment="Center">
        		<Button x:Name="btn_post" Content="post donation!" HorizontalAlignment="Right" Height="108" Margin="-11,0,-10,0" VerticalAlignment="Bottom" Width="477" Background="#FFB82D47" BorderBrush="#FF801F31" FontSize="26.667" Style="{StaticResource nextBtn}" Click="btn_post_Click"/>
        		<StackPanel Orientation="Vertical" VerticalAlignment="Top" HorizontalAlignment="Center" Width="456" Background="#FF3D7391" Margin="0,38,0,0" Height="520">
        			<TextBlock TextWrapping="Wrap" FontSize="32" Margin="10,0" Text="Address" FontStyle="Italic" Foreground="#FFB1E0FF" Padding="0,10,0,0"/>
        			<CheckBox x:Name="checkboxAddress" Content="Use Registered Address" Margin="150,-55,0,0" Checked="loadAddress" Unchecked="clearAddressField"/>
        			<TextBox x:Name="tf_address" Height="125" TextWrapping="Wrap" Margin="0,-20,0,0"/>
        			<TextBlock TextWrapping="Wrap" FontSize="32" Margin="10,0" Text="Time Available" FontStyle="Italic" Foreground="#FFB1E0FF"/>
        			<TextBox x:Name="tf_time" Height="125" TextWrapping="Wrap"/>
        			<TextBlock TextWrapping="Wrap" FontSize="32" Margin="10,0" Text="Special Instruction" FontStyle="Italic" Foreground="#FFB1E0FF"/>
        			<TextBox x:Name="tf_special_instruction" Height="125" TextWrapping="Wrap"/>
        		</StackPanel>
        	</Grid>
        </Grid>
        <Rectangle x:Name="blackeffect" Fill="Black" Stroke="Black" Opacity="0.68" Grid.RowSpan="2" Visibility="Collapsed"/>
        <toolkit:CustomMessageBox x:Name="popup" HorizontalAlignment="Center" Margin="10,191,10,189" VerticalAlignment="Center" Background="#FF191919" Width="460" d:IsHidden="True">
        	<StackPanel HorizontalAlignment="Left" Height="370" VerticalAlignment="Top" Width="450">
        		<Grid Height="305" Margin="0,0,10,0">
        			<TextBlock x:Name="gettingReadytb" HorizontalAlignment="Center" Margin="0,115,0,0" TextWrapping="Wrap" Text="Getting it ready" VerticalAlignment="Top" FontSize="32" Visibility="Collapsed"/>
        			<ProgressBar x:Name="progressBar" Margin="0,0,0,-100" VerticalAlignment="Center" Visibility="Collapsed" IsIndeterminate="True"/>
        			<TextBlock x:Name="rulestb" HorizontalAlignment="Left" TextWrapping="Wrap" Foreground="White" Margin="13,0,0,0">
        				<Run FontSize="48" Text="Rules"/>
        				<LineBreak/>
        				<Run Text="*Limit each donation to at most 3 big boxes for"/>
        				<LineBreak/>
        				<Run Text="  the ease of volunteers' pickup."/>
        				<LineBreak/>
        				<Run Text="*Separate construction supplies &amp; equipments"/>
        				<LineBreak/>
        				<Run Text="  from donations that can be consumed."/>
        				<LineBreak/>
        				<Run Text="*This app is to help the community, please do "/>
        				<LineBreak/>
        				<Run Text="  refrain from posting fake donations that may "/>
        				<LineBreak/>
        				<Run Text="  affect the relief effort processes."/>
        				<LineBreak/>
        				<Run/>
        				<LineBreak/>
        				<Run Text="Thank you"/>  
        			</TextBlock>
        		</Grid>
        		<Button x:Name="btn_agree" Content="I Agree" Style="{StaticResource PopUpBtn}" VerticalAlignment="Bottom" Margin="0,0,10,0" Click="btn_agree_Click"/>
        	</StackPanel>
        </toolkit:CustomMessageBox>
        <Grid x:Name="qtypopup" Margin="27,159,27,336" VerticalAlignment="Center" Background="#FF52ADEA" Height="273" RenderTransformOrigin="0.5,0.5" d:IsHidden="True">
        	<Grid.RenderTransform>
        		<CompositeTransform/>
        	</Grid.RenderTransform>
        	<Image x:Name="qtypopup_img" HorizontalAlignment="Left" Height="139" Margin="10,53,0,20" VerticalAlignment="Top" Width="139" Source="/Assets/AppBar/landmarks.png"/>
        	<TextBlock x:Name="qtypopup_question" HorizontalAlignment="Center" Margin="0,5,0,0" TextWrapping="Wrap" Text="How many pieces?" VerticalAlignment="Top" FontSize="36"/>
        	<TextBlock x:Name="qtypopup_itemtb" HorizontalAlignment="Left" Margin="160,63,0,0" TextWrapping="Wrap" Text="Construction Supplies" VerticalAlignment="Top" FontSize="22.667" Foreground="#FF1D5072" FontWeight="Bold"/>
        	<TextBox x:Name="qtypopup_qty" Height="99" Margin="0,93,10,81" TextWrapping="Wrap" Text="0" VerticalAlignment="Center" Width="150" TextAlignment="Center" HorizontalAlignment="Right" FontSize="48" Foreground="#FF224C68" SelectionBackground="#FF346CA4" KeyUp="qtypopup_qty_KeyUp" VerticalScrollBarVisibility="Disabled" InputScope="Number"/>
        	<Button x:Name="qtypopup_btn" Content="OK" HorizontalAlignment="Center" Margin="0,192,0,0" VerticalAlignment="Top" Height="81" Width="426" Background="#FF0A4C78" BorderBrush="#FF06304B" Click="exitWhatItem"/>
        </Grid>
        <toolkit:CustomMessageBox x:Name="popuperr" HorizontalAlignment="Center" Margin="10,191,10,189" VerticalAlignment="Center" Background="#FF191919" Width="460" Visibility="Collapsed">
        	<StackPanel HorizontalAlignment="Left" Height="370" VerticalAlignment="Top" Width="450">
        		<Grid Height="305" Margin="0,0,10,0">
        			<TextBlock x:Name="popuperrmsg" TextWrapping="Wrap" Foreground="White" FontSize="32" Text="You must donate something!" VerticalAlignment="Center" TextAlignment="Center"/>
        		</Grid>
        		<Button x:Name="btn_agree1" Content="ok" Style="{StaticResource PopUpBtn}" VerticalAlignment="Bottom" Margin="0,0,10,0" Click="popuperrclose"/>
        	</StackPanel>
        </toolkit:CustomMessageBox>
    </Grid>

</phone:PhoneApplicationPage>